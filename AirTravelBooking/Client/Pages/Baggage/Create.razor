@page "/Baggages/Create"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3 class="card-title">Create New Priorities</h3>
<hr />
<br />

<FormComponent ButtonText="Update" OnValidSubmit="@CreateBaggage" baggage="baggage" />

@code {
    Baggage baggage = new Baggage();

    private async Task CreateBaggage()
    {
        await _client.PostAsJsonAsync(EndPoints.AirplanesEndPoint, baggage);
        _navManager.NavigateTo("/Baggage");
    }

}
